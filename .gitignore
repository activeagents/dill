# Ignore storage files (including databases and uploaded files)
/storage/*
!/storage/.keep

# Ignore node modules
/node_modules

# Ignore bundled gems
/vendor/bundle

# Ignore DS_Store files
.DS_Store

# Ignore IDE files
.idea
.vscode
*.swp
*.swo
*~

# Ignore Playwright MCP files
.playwright-mcp/*
